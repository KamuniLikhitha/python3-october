import bisect
class Solution:
    def avoidFlood(self, rains: List[int]) -> List[int]:
        n = len(rains)
        ans = [-1]*n
        lake_full = {}
        dry_days = []
        for i, lake in enumerate(rains):
            if lake == 0:
                dry_days.append(i)
                ans[i] = 1
            else:
                if lake in lake_full:
                    j = bisect.bisect_right(dry_days, lake_full[lake])
                    if j == len(dry_days):
                        return []
                    dry_day = dry_days.pop(j)
                    ans[dry_day] = lake
                lake_full[lake] = i
                ans[i] = -1
        return ans
